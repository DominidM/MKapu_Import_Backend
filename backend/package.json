{
  "name": "mkapu-import-backend",
  "version": "1.0.0",
  "description": "Backend para Sistema de Inventario",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node api-gateway/index.js",
    "dev": "nodemon api-gateway/index.js",
    "start:auth": "node microservices/auth/src/index.js",
    "dev:auth": "nodemon microservices/auth/src/index.js",
    "dev:admin": "nodemon microservices/administracion/services/src/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:auth\" \"npm run dev:admin\""
  },
  "keywords": [
    "inventory",
    "express",
    "mysql"
  ],
  "author": "DominidM",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.16.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11"
  }
}